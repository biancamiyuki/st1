

#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	float numBoxSold;
	float numSidelineSold;
	float numPremiumSold;
    float numGenAdSold;
	cout << "GAME 1" << endl;


	cout << "Ticket price of General Admission = 25 " << endl;
	cout << "Number of General Admission Tickets sold = ";
	cin >> numGenAdSold; 
	cout << "Total Sales of General Admission Tickets = " << numGenAdSold * 25 << endl; 
	cout << endl;

	cout << "Ticket price of Sideline = 50 " << endl;
	cout << "Number of Sideline Tickets sold = ";
	cin >> numSidelineSold; 
	cout << "Total Sales of Sideline Tickets = " << numSidelineSold * 50 << endl;
	cout << endl;

	cout << "Ticket price of Premium = 100 " << endl;
	cout << "Number of Premium Tickets sold = ";
	cin >> numPremiumSold; 
	cout << "Total Sales of Premium Tickets = " << numPremiumSold * 100 << endl; 
	cout << endl;

	cout << "Ticket price of Box = 250 " << endl;
	cout << "Number of Box Tickets sold = ";
	cin >> numBoxSold; 
	cout << "Total Sales of Box Tickets = " << numBoxSold * 250 << endl; 
	cout << endl;

	
	cout << "Total Sales of Game 1 = " << (numBoxSold * 250) + (numPremiumSold * 100) + (numSidelineSold * 50) + (numGenAdSold * 25) << endl; 
	cout << "  " << endl;


